    <!-- Chart Logic -->
    <script>
        // --- Gemini AI Logic ---
        async function getRecommendation() {
            const input = document.getElementById('userProjectDescription').value;
            if (!input.trim()) {
                alert("Please describe your project first so Gemini can help!");
                return;
            }
            
            const btn = document.getElementById('recommendBtn');
            const responseDiv = document.getElementById('aiResponse');
            const responseText = document.getElementById('aiResponseText');
            
            // Loading State
            btn.disabled = true;
            btn.innerHTML = `<svg class="animate-spin h-5 w-5 mr-2 inline-block text-white" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"><circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle><path class="opacity-75" fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path></svg> Analyzing with Gemini...`;
            responseDiv.classList.add('hidden');
            
            // Context from the infographic for the AI
            const context = `
            You are a Senior Frontend Architect. Use the following analysis of React i18n libraries to recommend the best choice for the user's project.
            
            Data Comparison:
            - react-i18next: Market leader (4.2M downloads), Heavy bundle (14kb), Excellent Ecosystem, Runtime processing. Best for: Large, dynamic apps needing flexibility.
            - react-intl: Enterprise standard, Medium bundle (12kb), ICU Syntax, CLI extraction. Best for: Enterprise/Banking apps.
            - LinguiJS: Performance focused, Light bundle (5kb), Compiled catalogs (ICU), Native extraction. Best for: Performance critical apps.
            - typesafe-i18n: TypeScript focused, Micro bundle (<1kb), Simple functions, Generator based. Best for: Strict TypeScript apps where bundle size is critical.
            
            User's Project Description: "${input}"
            
            Task: Recommend ONE library from the list above. Explain why in 2-3 concise sentences based on the user's constraints (e.g. team size, typescript usage, performance needs). Bold the recommended library name.
            `;

            try {
                const apiKey = ""; // Provided by environment
                const response = await fetch(`https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash-preview-09-2025:generateContent?key=${apiKey}`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        contents: [{ parts: [{ text: context }] }]
                    })
                });
                
                if (!response.ok) throw new Error('API Request Failed');

                const data = await response.json();
                const markdownText = data.candidates?.[0]?.content?.parts?.[0]?.text || "Could not generate recommendation. Please try again.";
                
                // Simple formatting
                responseText.innerHTML = markdownText
                    .replace(/\*\*(.*?)\*\*/g, '<strong class="text-indigo-400">$1</strong>')
                    .replace(/\n/g, '<br>');
                    
                responseDiv.classList.remove('hidden');
                
            } catch (e) {
                console.error(e);
                alert("Sorry, there was an error connecting to the AI advisor. Please try again later.");
            } finally {
                btn.disabled = false;
                btn.innerHTML = `<span>✨</span> Get AI Recommendation`;
            }
        }

        // --- UTILITY: Label Wrapping Logic (>16 chars) ---
        function wrapLabels(labels) {
            return labels.map(label => {
                if (typeof label === 'string' && label.length > 16) {
                    const words = label.split(' ');
                    const lines = [];
                    let currentLine = words[0];

                    for (let i = 1; i < words.length; i++) {
                        if ((currentLine + " " + words[i]).length <= 16) {
                            currentLine += " " + words[i];
                        } else {
                            lines.push(currentLine);
                            currentLine = words[i];
                        }
                    }
                    lines.push(currentLine);
                    return lines;
                }
                return label;
            });
        }

        // --- UTILITY: Tooltip Configuration ---
        const commonTooltipOptions = {
            callbacks: {
                title: function(tooltipItems) {
                    const item = tooltipItems[0];
                    let label = item.chart.data.labels[item.dataIndex];
                    if (Array.isArray(label)) {
                        return label.join(' ');
                    } else {
                        return label;
                    }
                }
            }
        };

        // --- CHART 1: Market Share (Bar) ---
        const ctxAdoption = document.getElementById('adoptionChart').getContext('2d');
        const rawLabelsAdoption = ['react-i18next (Market Leader)', 'react-intl (FormatJS)', 'LinguiJS (Compiled)', 'typesafe-i18n (Lightweight)'];
        
        new Chart(ctxAdoption, {
            type: 'bar',
            data: {
                labels: wrapLabels(rawLabelsAdoption),
                datasets: [{
                    label: 'Weekly NPM Downloads (Approx. Millions)',
                    data: [4.2, 1.3, 0.25, 0.04],
                    backgroundColor: [
                        'rgba(37, 99, 235, 0.8)', // Blue
                        'rgba(219, 39, 119, 0.8)', // Pink
                        'rgba(16, 185, 129, 0.8)', // Emerald
                        'rgba(147, 51, 234, 0.8)'  // Purple
                    ],
                    borderColor: [
                        'rgba(37, 99, 235, 1)',
                        'rgba(219, 39, 119, 1)',
                        'rgba(16, 185, 129, 1)',
                        'rgba(147, 51, 234, 1)'
                    ],
                    borderWidth: 1
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: { display: false },
                    tooltip: commonTooltipOptions
                },
                scales: {
                    y: {
                        beginAtZero: true,
                        title: { display: true, text: 'Weekly Downloads (Millions)' }
                    }
                }
            }
        });

        // --- CHART 2: Performance Matrix (Scatter/Bubble) ---
        // X: Bundle Size (smaller is better - inverted logic conceptually, but plotted linear)
        // Y: Developer Satisfaction / Flexibility
        // R: Complexity/Feature Set
        const ctxPerformance = document.getElementById('performanceChart').getContext('2d');
        
        new Chart(ctxPerformance, {
            type: 'bubble',
            data: {
                datasets: [
                    {
                        label: 'react-i18next',
                        data: [{x: 14, y: 90, r: 25}], // Big bundle, High Satisfaction, Feature Rich
                        backgroundColor: 'rgba(37, 99, 235, 0.7)',
                        borderColor: 'rgba(37, 99, 235, 1)'
                    },
                    {
                        label: 'react-intl',
                        data: [{x: 12, y: 75, r: 20}],
                        backgroundColor: 'rgba(219, 39, 119, 0.7)',
                        borderColor: 'rgba(219, 39, 119, 1)'
                    },
                    {
                        label: 'LinguiJS',
                        data: [{x: 5, y: 85, r: 15}], // Small bundle, Good Satisfaction
                        backgroundColor: 'rgba(16, 185, 129, 0.7)',
                        borderColor: 'rgba(16, 185, 129, 1)'
                    },
                    {
                        label: 'typesafe-i18n',
                        data: [{x: 1, y: 80, r: 12}], // Tiny bundle, Good Satisfaction
                        backgroundColor: 'rgba(147, 51, 234, 0.7)',
                        borderColor: 'rgba(147, 51, 234, 1)'
                    }
                ]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    tooltip: {
                        callbacks: {
                            label: function(context) {
                                return context.dataset.label + ': ' + context.raw.x + 'kB Bundle Size';
                            }
                        }
                    },
                    legend: {
                        position: 'bottom'
                    }
                },
                scales: {
                    x: {
                        title: { display: true, text: 'Bundle Overhead (kB) - Lower is Better for Mobile' },
                        min: 0,
                        max: 18,
                        reverse: true // Visual trick: Right side = Smaller bundle (Better)
                    },
                    y: {
                        title: { display: true, text: 'Developer Satisfaction Score' },
                        min: 60,
                        max: 100
                    }
                }
            }
        });

        // --- CHART 3: Feature Radar ---
        const ctxRadar = document.getElementById('radarChart').getContext('2d');
        const radarLabels = wrapLabels(['Type Safety', 'Runtime Perf', 'Documentation', 'Ecosystem Plugins', 'Ease of Setup']);

        new Chart(ctxRadar, {
            type: 'radar',
            data: {
                labels: radarLabels,
                datasets: [
                    {
                        label: 'react-i18next',
                        data: [70, 75, 95, 100, 85], // Strong docs/ecosystem, avg types
                        borderColor: 'rgba(37, 99, 235, 1)',
                        backgroundColor: 'rgba(37, 99, 235, 0.2)',
                        pointBackgroundColor: 'rgba(37, 99, 235, 1)',
                    },
                    {
                        label: 'typesafe-i18n',
                        data: [100, 95, 80, 50, 70], // Perfect types/perf, smaller ecosystem
                        borderColor: 'rgba(147, 51, 234, 1)',
                        backgroundColor: 'rgba(147, 51, 234, 0.2)',
                        pointBackgroundColor: 'rgba(147, 51, 234, 1)',
                    }
                ]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    tooltip: commonTooltipOptions,
                    legend: { position: 'top' }
                },
                scales: {
                    r: {
                        angleLines: {
                            display: true
                        },
                        suggestedMin: 0,
                        suggestedMax: 100
                    }
                }
            }
        });
    </script>
</body>
</html>
